"typography": {
  "headlines": {
    "font": "Khand",
    "transform": "uppercase",
    "fallbacks": ["Oswald", "Impact", "sans-serif"],
    "weights": {
      "h1-h4": 700,
      "h5-h6": 600
    }
  },
  "body": {
    "font": "General Sans",
    "fallbacks": ["Public Sans", "Open Sans", "Tahoma", "sans-serif"],
    "weights": [300, 400],
    "context": "Use 300/400 on dark backgrounds"
  },
  "cdn": "https://cdn.jsdelivr.net/gh/REVREBEL/fonts@main/rebel-fonts.css"
}